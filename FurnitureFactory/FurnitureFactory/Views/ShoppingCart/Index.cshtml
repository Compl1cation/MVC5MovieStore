@using FurnitureFactory.Models.ViewModels
@model IEnumerable<ProductQuantityViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Shopping Cart</h2>

<table>
    <thead>Product: </thead>>
    <thead>Quantity: </thead>>
    @foreach (var item in Model)
    {
    <tr>
        <td>@Html.DisplayFor(listitem => item.ProductId)</td>
        <td>@Html.DisplayFor(listitem => item.Quantity)</td>
    </tr>
    }
</table>

