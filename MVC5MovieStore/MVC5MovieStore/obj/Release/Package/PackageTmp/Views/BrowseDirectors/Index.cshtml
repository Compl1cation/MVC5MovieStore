@model IEnumerable<MVC5MovieStore.Models.Director>
@using System.Data.Entity;
@using System.Linq;

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Browse by directors:</h2>

@{
    foreach (var director in Model)
    {
        @Html.ActionLink(@director.Name, "Details", new { id = director.DirectorId }, new {@class="links"})<br />
    }
}
