@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>
@using System.Data.Entity;
@using System.Linq;

@{
    ViewBag.Title = "User Control Panel";
    SortingInfo info = ViewBag.SortingInfo;
}
<script src="/Scripts/jquery-1.4.4.min.js" type="text/javascript"></script>
<script type="text/javascript" src="@Url.Content("/Scripts/MyScripts.js")"></script>

<h2>Role Manager</h2>

@Html.ActionLink("Create Role", "Create", "RoleManager")

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id =item.Id }, new { @id = "actionp" }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }, new { @id = "actionp" }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { @id = "actionp" })
            </td>
        </tr>
    }

</table>